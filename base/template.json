    "scripts": {
      "check-types": "tsc --noEmit --noUnusedLocals",
      "fix": "npm-run-all fix:*",
      "fix:eslint": "npm run lint:eslint -- --fix",
      "fix:prettier": "prettier \"src/**/*.js,jsx,ts,tsx,scss}\" --write",
      "generate": "sg wizard",
      "lint": "npm-run-all lint:*",
      "lint:eslint": "eslint ./src --ext .js,.jsx,.ts,.tsx --cache",
